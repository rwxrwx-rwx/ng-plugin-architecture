{
  "root": "libs/generators",
  "sourceRoot": "libs/generators/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/generators",
        "tsConfig": "libs/generators/tsconfig.lib.json",
        "packageJson": "libs/generators/package.json",
        "main": "libs/generators/src/index.ts",
        "assets": ["libs/generators/*.md"]
      }
    },
    "runtime": {
      "executor": "@nrwl/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/generators",
        "tsConfig": "libs/generators/tsconfig.lib.json",
        "packageJson": "libs/generators/package.json",
        "main": "libs/generators/src/index.ts",
        "assets": [],
        "configurations": {
          "production": {
            "optimization": true,
            "extractLicenses": true,
            "inspect": false
          }
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:execute",
      "options": {
        "buildTarget": "generators:runtime",
        "inspect": "inspect-brk",
        "port": 9229
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/generators/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/generators"],
      "options": {
        "jestConfig": "libs/generators/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
